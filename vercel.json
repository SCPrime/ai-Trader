{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/ai-query",
      "dest": "app.py"
    },
    {
      "src": "/ai-chat-enhanced",
      "dest": "app.py"
    },
    {
      "src": "/ws/ai-enhanced",
      "dest": "app.py"
    },
    {
      "src": "/dashboard",
      "dest": "app.py"
    },
    {
      "src": "/supervisor",
      "dest": "app.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "FORCE_PAPER_TRADING": "true"
  }
}