{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "api/health.py"
    },
    {
      "src": "/api/strategy/test",
      "dest": "api/strategy/test.py"
    },
    {
      "src": "/api/strategy/create",
      "dest": "api/strategy/create.py"
    },
    {
      "src": "/api/strategy/analyze",
      "dest": "api/strategy/analyze.py"
    },
    {
      "src": "/api/portfolio/positions",
      "dest": "api/portfolio/positions.py"
    },
    {
      "src": "/api/trading/execute",
      "dest": "api/trading/execute.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}